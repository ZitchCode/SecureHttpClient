<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net9.0-android35.0</TargetFrameworks>
		<IsAotCompatible>true</IsAotCompatible>
		<SupportedOSPlatformVersion Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'android'">21.0</SupportedOSPlatformVersion>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Square.OkHttp3.Android" Version="5.1.0.2" />
		<PackageReference Include="Xamarin.Brotli.Dec" Version="0.1.2.9" />
	</ItemGroup>

	<ItemGroup>
		<AndroidLibrary Include="Jars\securehttpclient-okhttp.jar" />
		<AndroidLibrary Include="Jars\securehttpclient-javanetcookiejar.jar" />
	</ItemGroup>

	<ItemGroup>
		<AndroidJavaSource Remove="java\**" />
		<AndroidJavaSource Remove="kt\**" />
	</ItemGroup>
  
</Project>
